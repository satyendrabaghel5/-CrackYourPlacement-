//climbing stairs

class Solution {
public:
int f(int n, vector<int>&dp){
    
   
if(n==1 || n==0){
        return 1;
    }
    if(dp[n-1]!=-1)
        return dp[n-1];
    
    dp[n-1]=f(n-1,dp)+f(n-2,dp);
    return dp[n-1];
        
}
    int climbStairs(int n) {
        vector<int>dp(n+1,-1);
        return f(n,dp);
    }
};
